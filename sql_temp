	[Image] [nvarchar](200) NULL,
	[Click] [int] NOT NULL,
	[Stock] [int] NOT NULL,
	[SellCount] [int] NOT NULL,
	[RetailPrice] [decimal](18, 2) NOT NULL,
	[Price] [decimal](18, 2) NOT NULL,
	[Sort] [int] NOT NULL,
